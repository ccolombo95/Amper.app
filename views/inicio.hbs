<h1>Bienvenidos a Educación IT</h1>
<hr>

<h2>Listado de productos</h2>

<table class="table">
  <thead>
    <tr>
      <th scope="col">Nombre</th>
      <th scope="col">Categoría</th>
      <th scope="col">Disponible</th>
      <th scope="col">Stock</th>
      <th scope="col">Precio</th>
    </tr>
  </thead>
  <tbody>
    
    {{#each productos}}

        <tr>
            <td scope="row">{{this.nombre}}</td>
            <td>{{this.categoria}}</td>
            <td>{{this.disponible}}</td>
            <td>{{this.stock}}</td>
            <td>{{this.precio}}</td>
        </tr>

    {{/each}}
  </tbody>
</table>